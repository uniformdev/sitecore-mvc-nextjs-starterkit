{
  "name": "@uniformdev/habitat92-site-next",
  "version": "2.0.0",
  "publishConfig": {
    "registry": "https://pkgs.dev.azure.com/altola/_packaging/sitelify/npm/registry/"
  },
  "private": true,
  "license": "SEE LICENSE IN PROVIDED DISTRIBUTION PACKAGE",
  "scripts": {
    "build": "next build",
    "start": "next build && cross-env NODE_ENV=production node ./server.js",
    "format": "prettier --write \"{./components,./pages}/**/*.{js,ts,tsx}\"",
    "export:site": "next export --threads 1",
    "move:static": "node ./scripts/move-static.js",
    "export": "npm-run-all --serial build export:site move:static",
    "azure": "node ./deploy-azure.js",
    "aws": "node ./deploy-aws.js",
    "netlify": "node ./deploy-netlify.js",
    "deploy:azure": "npm-run-all --serial export azure",
    "deploy:aws": "npm-run-all --serial export aws",
    "deploy:netlify": "npm-run-all --serial export netlify"
  },
  "dependencies": {
    "@uniformdev/common": "^2.0.200402-1",
    "@uniformdev/common-client": "^2.0.200402-1",
    "@uniformdev/next": "^2.0.200402-1",
    "@uniformdev/next-server": "^2.0.200402-1",
    "cross-env": "^7.0.2",
    "dotenv": "^8.2.0",
    "loglevel": "^1.6.3",
    "next": "9.3.2",
    "next-compose-plugins": "^2.2.0",
    "react": "^16.13.1",
    "react-content-loader": "^5.0.2",
    "react-dom": "^16.13.1",
    "typescript": "^3.5.1",
    "winston": "^3.2.1"
  },
  "devDependencies": {
    "@types/dotenv": "^8.2.0",
    "@types/react": "^16.8.17",
    "@types/react-dom": "^16.8.4",
    "@uniformdev/publishing-awss3": "^2.0.200402-1",
    "@uniformdev/publishing-azureblobstorage": "^2.0.200402-1",
    "@uniformdev/publishing-netlify": "^2.0.200402-1",
    "mv": "^2.1.1",
    "npm-run-all": "^4.1.5"
  }
}
